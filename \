#include <stdio.h>
#include <sys/mman.h>
#include <unistd.h>
#include <string.h>

int	main()
{
	unsigned	char	*data;
	data = NULL;

	if ((data = mmap(NULL, 4096, PROT_EXEC | PROT_WRITE | PROT_READ, MAP_ANONYMOUS, -1, 0)))
	{
		data[0] = 'F';
		data[1] = 0;
		printf("Youpi:\n", data);
	}
	else
	{
		printf("ooooooh! ...\n");
	}
	return (0);
}
